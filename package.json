    "#locales/*": "./src/locales/*",
    "#assets/*": "./src/assets/*",
    "#layouts/*": "./src/layouts/*",
    "#utils/*": "./src/utils/*",
    "#stores/*": "./src/stores/*",
    "#renderer/*": "./src/renderer/*",
